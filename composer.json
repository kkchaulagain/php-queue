{
    "name": "kkchaulagain/php-queue",
    "description": "Php rabbit queue",
    "license": "MIT",
    "authors": [
        {
            "name": "Kiran Chaulagain",
            "email": "kkchaulagain@email.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4 | ^8.0",
        "php-amqplib/php-amqplib": "^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.17",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "kkchaulagain\\phpQueue\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kkchaulagain\\phpQueue\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor\\bin\\phpunit",
        "phpunit": "vendor/bin/phpunit",
        "coverage-test": "vendor\\bin\\phpunit --coverage-html /test-reports/coverage",
        "format": "vendor\\bin\\php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
